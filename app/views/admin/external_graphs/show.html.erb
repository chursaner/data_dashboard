<% if @external_graph.page && @external_graph.page.category %>
  <h1 class="text-center"><%= link_to @external_graph.page.category.name, admin_category_path(@external_graph.page.category) %></h1>
<% end %>
<% if @external_graph.page %>
  <h1 class="text-center"><%= link_to @external_graph.page.name, admin_page_path(@external_graph.page) %></h1>
<% end %>

<h1 class="text-center"><%= "Data for #{@external_graph.title}" %></h1>

<iframe class="iframes" src=<%= @external_graph.external_url %>></iframe>
